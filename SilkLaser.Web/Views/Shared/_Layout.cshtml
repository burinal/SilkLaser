    <script src="/Content/html3/assets/js/libs.js"></script>
    <script src="/Content/html3/assets/js/scripts.js"></script>
    <script>
        $(document).ready(function () {
            $('.page-preloader .loader').stop(true, true).delay(2000).animate({ opacity: '0' }, 200, 'easeOutExpo', function () {
                document.body.classList.remove('loading');
                $('.page-preloader').fadeOut('slow');
            });

            $(".sbmt").click(function () {
                var phone = $("#phone-field").val();
                if (phone === null || phone === undefined || phone.trim() === "") {
                    alert("Введите ваш телефон");
                    return;
                }

                $.post("/feedback", { p: phone }, function (result) {
                    if (result.success === true) {
                        alert("Ваша заявка успешно отправлена");
                    }
                }, 'json');

            });
        });
    </script>
