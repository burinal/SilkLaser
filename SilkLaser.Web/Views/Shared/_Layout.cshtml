    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/Content/html3/assets/css/libs.css" />
    <link rel="stylesheet" href="/Content/html3/assets/css/style.css" />
    <link rel="icon" type="image/png" href="/Content/html3/assets/img/favicon.png"/>
    <style>
        body.loading {
            overflow: hidden;
        }

        .page-preloader {
            position: fixed;
            left: 0;
            top: 0;
            right: 0;
            bottom: 0;
            background: #fff;
            overflow: hidden;
            z-index: 100500;
        }

        .loader {
            display: inline-block;
            line-height: 0;
            top: 50%;
            left: 50%;
            width: 160px;
            margin-top: -25px;
            height: 42px;
            position: absolute;
            transform: translateX(-50%);
        }

        .spinner-container {
            position: absolute;
            bottom: -20px;
            left: 50%;
            margin-left: -80px;
            width: 160px;
            height: 1px;
            overflow: hidden;
        }

            .spinner-container:before {
                content: '';
                position: absolute;
                top: 0;
                width: 100%;
                height: 1px;
                transform: translate(-100%, 0);
                background-color: #bc9e52;
                animation: loading 3400ms cubic-bezier(.23, 1, .32, 1) infinite;
                animation-delay: 2s;
            }

        @@keyframes loading {
            75% {
                transform: translate(0, 0);
            }

            100% {
                transform: translate(100%, 0);
            }
        }
    </style>
</head>
<body class="loading">
    <div class="page-preloader">
        <div class="loader">
            <div class="logo">
                <img src="/Content/html3/assets/img/logo.png" alt="img">
            </div>
